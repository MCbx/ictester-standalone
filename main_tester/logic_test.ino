  passNo=0;
  stepNo=0;
 byte k='o';
  while (k!='s')
  {
    Serial.println("    74  TEST");
    Serial.print("   < ");
    Serial.print(pgm_read_dword_near(chips1+menuptr1));
    Serial.println(" >");
    while (Serial.available()==0);
    k=Serial.read();
    if (k=='a')
    {
        menuptr1--;
        if (menuptr1==255)
            menuptr1=chips1count-1;
    }
    if (k=='d') //forward
    {
        menuptr1++;
        if (menuptr1==chips1count)
            menuptr1=0;
    }
  }

  Serial.println(" RUNNING...");
  if (menuptr1==74)
  {
    powerOn();
    for (byte kk=0;kk<100;kk++)
    {
       test4040();
       passNo++;
